{
  "name": "eustia",
  "version": "0.6.2",
  "description": "Tool for generating utility libraries",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eustia.git"
  },
  "bin": {
    "eustia": "./bin/eustia.js"
  },
  "scripts": {
    "ci": "npm run lint && npm test",
    "test": "mocha",
    "lint": "eslint lib cmd bin",
    "cov": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly"
  },
  "eustia": {
    "output": "lib/util.js",
    "files": [
      "index.js",
      "bin/eustia.js",
      "cmd/**/*.js",
      "lib/*.js"
    ],
    "format": "commonjs"
  },
  "keywords": [
    "utility",
    "library",
    "custom",
    "javascript",
    "underscore"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eustia/issues"
  },
  "homepage": "https://github.com/liriliri/eustia#readme",
  "dependencies": {
    "async": "^2.6.0",
    "chalk": "^2.3.0",
    "chokidar": "^2.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "marked": "^0.3.9",
    "nopt": "^4.0.1",
    "qs": "^6.5.1",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0"
  }
}
